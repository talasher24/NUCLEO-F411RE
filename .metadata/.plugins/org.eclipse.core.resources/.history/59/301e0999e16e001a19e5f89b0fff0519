#include "Commands.h"

void whichCommand (void)
{
	for (uint8_t i = 0; i < NUM_OF_COMMANDS; i++)
	{
		if (strncmp((char*)s_buffer._p_rx_buffer, commands._name, commands[i]._size)==0)
		{
			commands[i].func_ptr(i);
		}
	}
}

void ping_callBack(uint8_t i)
{
	HAL_UART_Transmit(&huart2, commands[i]._name, commands[i]._size, 10);
}

void version_callback(uint8_t i)
{
	HAL_UART_Transmit(&huart2, commands[i]._name, commands[i]._size, 10);
}
