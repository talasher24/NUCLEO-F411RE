#include "stm32f4xx_hal.h"

void ping_callBack(char* token)
{
	uart_print(token);
}

void version_callback(char* token)
{
	uart_print(VERSION);
}

void pwm_start_callback(char* token)
{
	HAL_TIM_PWM_Start(&htim3, TIM_CHANNEL_1);
	uart_print(OK);
}



